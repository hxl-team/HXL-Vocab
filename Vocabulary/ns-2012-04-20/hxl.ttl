hxl:Population
        a rdfs:Class ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Population"@en ;
        rdfs:subClassOf foaf:Group ;
        rdfs:comment "Generic class for any kind of population. Example: Total population, affected population etc; use the subclasses for the data annotation."@en .

hxl:TotalPopulation
        a rdfs:Class ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Total Population"@en ;
        rdfs:subClassOf hxl:Population ;
        rdfs:comment "Total population for a given administrative unit or geographic area. Instances can be created to make statements about this population."@en .

hxl:AgeGroup
        a rdfs:Class ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Age Group"@en ;
        rdfs:subClassOf hxl:Population;
        rdfs:comment "Generic class for age groups; instances should be created for actual age groups. Example: <em>Children under 5</em>. " .
        
