        a rdfs:Class ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Displaced"@en ;
        skos:altLabel "Displacement"@en ;
        rdfs:subClassOf hxl:AffectedPopulation ;
