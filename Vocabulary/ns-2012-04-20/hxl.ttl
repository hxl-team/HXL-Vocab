hxl:IndicatorUnit
        a rdfs:Class ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Indicator unit"@en;
        rdfs:comment "Measurement unit for an indicator; Instances to be created in the registry: <em>Household, Individual, Session, School, TSS, Child, Student, Square Meter, Teacher, Child friendly spaces, Committee, Acre, Animal, Meeting, Hub, Tree, and 'other'</em>.<br /><small>Term origin: Indicator registry</small>" .

hxl:Indicator
        a rdfs:Class ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Indicator"@en;
        rdfs:comment "Concrete measurable reference for a concrete need; e.g., <em>5 litres of clean water per person and day</em>. The different subclasses should be used to type instances.<br /><small>Term origin: Pakistan SRF, ACAPS DSS, CCDI, CERF, HPM Ethiopia, Sigmah DB, Indicator registry</small>" .
        
hxl:InputIndicator 
        a rdfs:Class ;
        rdfs:subClassOf hxl:Indicator ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Input indicator"@en;
rdfs:comment "<em>Explanation t.b.d.</em>.<br /><small>Term origin: Indicator registry.</small>" .

hxl:ProxyInputIndicator
        a rdfs:Class ;
        rdfs:subClassOf hxl:Indicator ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Proxy input indicator"@en;
rdfs:comment "<em>Explanation t.b.d.</em>.<br /><small>Term origin: Indicator registry.</small>" .

hxl:OutputIndicator 
        a rdfs:Class ;
        rdfs:subClassOf hxl:Indicator ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Output indicator"@en;
rdfs:comment "<em>Explanation t.b.d.</em>.<br /><small>Term origin: Indicator registry.</small>" .
        
hxl:OutcomeIndicator 
        a rdfs:Class ;
        rdfs:subClassOf hxl:Indicator ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Outcome indicator"@en;
rdfs:comment "<em>Explanation t.b.d.</em>.<br /><small>Term origin: Indicator registry.</small>" .

hxl:ProxyOutcomeIndicator
        a rdfs:Class ;
        rdfs:subClassOf hxl:Indicator ;
        rdfs:isDefinedBy hxl:situationSection ;
        skos:prefLabel "Proxy outcome indicator"@en;
rdfs:comment "<em>Explanation t.b.d.</em>.<br /><small>Term origin: Indicator registry.</small>" .
        

hxl:Injured
        a rdfs:Class ;
        rdfs:isDefinedBy hxl:situationSection  ;
        rdfs:subClassOf hxl:Casualty ;
        skos:prefLabel "Injured"@en;
        owl:disjointWith hxl:Death ;
        rdfs:comment "Indicates the Injured population.<br /><small>Term origin: ACAPS DSS, Humanitarian Profile, Shelter Cluster, HERAMS Checklist (<em>Injury</em>)</small>" .

